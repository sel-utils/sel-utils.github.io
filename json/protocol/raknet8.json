{

	"__software": "Raknet",
	"__protocol": 8,
	"__website": "https://github.com/sel-project/sel-utils",

	"encoding": {

		"endianness": {
			"triad": "little_endian",
			"*": "big_endian"
		},

		"id": "ubyte",

		"array_length": "ushort",

		"types": {

			"address": [
				{
					"name": "type",
					"type": "ubyte"
				},
				{
					"name": "ipv4",
					"type": "uint",
					"when": "type==4"
				},
				{
					"name": "ipv6",
					"type": "ubyte[16]",
					"when": "type==6"
				},
				{
					"name": "port",
					"type": "ushort"
				}
			],

			"acknowledge": [
				{
					"name": "unique",
					"type": "bool"
				},
				{
					"name": "first",
					"type": "triad"
				},
				{
					"name": "last",
					"type": "triad",
					"when": "unique==false"
				}
			],

			"encapsulation": [
				{
					"name": "info",
					"type": "ubyte"
				},
				{
					"name": "length",
					"type": "ushort"
				},
				{
					"name": "message_index",
					"type": "triad",
					"when": "(info&0x7F)>=64"
				},
				{
					"name": "order_index",
					"type": "triad",
					"when": "(info&0x7F)>=96"
				},
				{
					"name": "order_channel",
					"type": "ubyte",
					"when": "(info&0x7F)>=96"
				},
				{
					"name": "split",
					"type": "split",
					"when": "(info&0x10)!=0"
				},
				{
					"name": "payload",
					"type": "bytes"
				}
			],

			"split": [
				{
					"name": "count",
					"type": "uint"
				},
				{
					"name": "id",
					"type": "ushort"
				},
				{
					"name": "order",
					"type": "uint"
				}
			]

		}

	},

	"packets": {

		"control": {

			"ack": {
				"id": 192,
				"clientbound": true,
				"serverbound": true,
				"fields": [
					{
						"name": "packets",
						"type": "acknowledge[]"
					}
				]
			},

			"nack": {
				"id": 160,
				"clientbound": true,
				"serverbound": true,
				"fields": [
					{
						"name": "packets",
						"type": "acknowledge[]"
					}
				]
			},

			"encapsulated": {
				"id": 132,
				"clientbound": true,
				"serverbound": true,
				"fields": [
					{
						"name": "count",
						"type": "triad"
					},
					{
						"name": "encapsulation",
						"type": "encapsulation"
					}
				]
			}

		},

		"unconnected": {

			"ping": {
				"id": 1,
				"clientbound": false,
				"serverbound": true,
				"fields": [
					{
						"name": "ping_id",
						"type": "long"
					},
					{
						"name": "magic",
						"type": "ubyte[16]"
					}
				]
			},

			"pong": {
				"id": 28,
				"clientbound": true,
				"serverbound": false,
				"fields": [
					{
						"name": "ping_id",
						"type": "long"
					},
					{
						"name": "server_id",
						"type": "long"
					},
					{
						"name": "magic",
						"type": "ubyte[16]"
					},
					{
						"name": "status",
						"type": "string"
					}
				]
			},

			"open_connection_request_1": {
				"id": 5,
				"clientbound": false,
				"serverbound": true,
				"fields": [
					{
						"name": "magic",
						"type": "ubyte[16]"
					},
					{
						"name": "protocol",
						"type": "ubyte"
					},
					{
						"name": "mtu",
						"type": "bytes"
					}
				]
			},

			"open_connection_reply_1": {
				"id": 6,
				"clientbound": true,
				"serverbound": false,
				"fields": [
					{
						"name": "magic",
						"type": "ubyte[16]"
					},
					{
						"name": "server_id",
						"type": "long"
					},
					{
						"name": "security",
						"type": "bool"
					},
					{
						"name": "mtu_length",
						"type": "ushort"
					}
				]
			},

			"open_connection_request_2": {
				"id": 7,
				"clientbound": false,
				"serverbound": true,
				"fields": [
					{
						"name": "magic",
						"type": "ubyte[16]"
					},
					{
						"name": "server_address",
						"type": "address"
					},
					{
						"name": "mtu_length",
						"type": "ushort"
					},
					{
						"name": "client_id",
						"type": "long"
					}
				]
			},

			"open_connection_reply_2": {
				"id": 8,
				"clientbound": true,
				"serverbound": false,
				"fields": [
					{
						"name": "magic",
						"type": "ubyte[16]"
					},
					{
						"name": "server_id",
						"type": "long"
					},
					{
						"name": "client_address",
						"type": "address"
					},
					{
						"name": "mtu_length",
						"type": "ushort"
					},
					{
						"name": "security",
						"type": "bool"
					}
				]
			}

		},

		"encapsulated": {

			"client_connect": {
				"id": 9,
				"clientbound": false,
				"serverbound": true,
				"fields": [
					{
						"name": "client_id",
						"type": "long"
					},
					{
						"name": "ping_id",
						"type": "long"
					}
				]
			},

			"server_handshake": {
				"id": 16,
				"clientbound": true,
				"serverbound": false,
				"fields": [
					{
						"name": "client_address",
						"type": "address"
					},
					{
						"name": "mtu_length",
						"type": "ushort"
					},
					{
						"name": "system_addresses",
						"type": "address[10]"
					},
					{
						"name": "ping_id",
						"type": "long"
					},
					{
						"name": "server_id",
						"type": "long"
					}
				]
			},

			"client_handshake": {
				"id": 19,
				"clientbound": false,
				"serverbound": true,
				"fields": [
					{
						"name": "client_address",
						"type": "address"
					},
					{
						"name": "system_addresses",
						"type": "address[10]"
					},
					{
						"name": "ping_id",
						"type": "long"
					},
					{
						"name": "client_id",
						"type": "long"
					}
				]
			},

			"client_cancel_connection": {
				"id": 21,
				"clientbound": false,
				"serverbound": true,
				"fields": []
			},

			"ping": {
				"id": 0,
				"clientbound": false,
				"serverbound": true,
				"fields": [
					{
						"name": "time",
						"type": "long"
					}
				]
			},

			"pong": {
				"id": 3,
				"clientbound": true,
				"serverbound": false,
				"fields": [
					{
						"name": "time",
						"type": "long"
					}
				]
			},

			"mcpe": {
				"id": 254,
				"clientbound": true,
				"serverbound": true,
				"fields": [
					{
						"name": "packet",
						"type": "bytes"
					}
				]
			}

		}

	}

}
